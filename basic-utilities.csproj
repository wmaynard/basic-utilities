<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net9.0</TargetFramework>
        <LangVersion>13</LangVersion>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>disable</Nullable>
        <RootNamespace>Maynard</RootNamespace>
        <PackageId>maynard-basic-utilities</PackageId>
        <Title>Maynard Basic Utilities</Title>
        <Authors>Will Maynard</Authors>
        <Description>A small nuget package for commonly used logging functions in my projects.</Description>
        <Copyright>Will Maynard 2025</Copyright>
        <PackageProjectUrl>https://github.com/wmaynard/basic-utilities</PackageProjectUrl>
        <PackageLicenseUrl>https://github.com/wmaynard/basic-utilities/blob/main/LICENSE</PackageLicenseUrl>
        <RepositoryUrl>https://github.com/wmaynard/basic-utilities</RepositoryUrl>
        <Company>Will Maynard</Company>
        <Product>maynard-basic-utilities</Product>
        <AssemblyVersion>1.0.0</AssemblyVersion>
        <AssemblyName>basic-utilities</AssemblyName>
    </PropertyGroup>
    
  <ItemGroup>
    <FrameworkReference Include="Microsoft.AspNetCore.App" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="System.Text.Json" Version="9.0.9" />
  </ItemGroup>

</Project>
